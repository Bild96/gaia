{
  "name": "gaia-admin",
  "version": "0.0.1",
  "description": "Service for administrating a Gaia hub",
  "main": "lib/index.js",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "scripts": {
    "start": "npm run build && node lib/index.js",
    "start-dev": "ts-node src/index.ts",
    "clean": "tsc -b --clean tsconfig.build.json",
    "build": "npm run clean && tsc -b tsconfig.build.json && chmod +x lib/index.js",
    "lint": "eslint --ext .ts ./src",
    "test": "npm run lint && NODE_ENV=test nyc node ./test/test.ts"
  },
  "bin": {
    "blockstack-gaia-admin": "lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockstack/gaia"
  },
  "keywords": [
    "decentralized",
    "storage"
  ],
  "author": "Jude Nelson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/blockstack/gaia"
  },
  "dependencies": {
    "ajv": "^6.10.2",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-winston": "^3.3.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/express-winston": "^3.0.3",
    "@types/node": "^10.14.10",
    "@types/tape": "^4.2.33",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "eslint": "^6.1.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.13",
    "tape": "^4.11.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "homepage": "https://github.com/blockstack/gaia",
  "nyc": {
    "cache": false,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts"
    ],
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register"
    ],
    "reporter": [
      "text",
      "lcov"
    ]
  }
}

{
  "name": "gaia-storage",
  "version": "1.0.0",
  "description": "Client library for Blockstack Gaia",
  "main": "lib/index",
  "scripts": {
    "build": "npm run build-documentation && babel src/ -d lib/",
    "build-tests": "babel src/ -d lib/",
    "build-documentation": "true",
    "flow": "flow",
    "lint": "eslint src && eslint tests",
    "test": "npm run lint && npm run flow && npm run build && npm run build-tests && nyc --reporter=text npm run unit-test",
    "unit-test": "node ./tests/unitTests/lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blockstack/gaia.git"
  },
  "author": "Aaron Blankstein <aaron@blockstack.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "documentation": "^8.1.0",
    "eslint": "^5.3.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "flow-bin": "^0.78.0",
    "nyc": "^12.0.2",
    "tape": "^4.9.1"
  },
  "dependencies": {
    "blockstack": "^18.0.4",
    "cross-fetch": "^2.2.2",
    "jsontokens": "^0.7.8"
  }
}
